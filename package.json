{
  "name": "vue-code-view",
  "version": "0.4.1",
  "description": "A Vue 2 component like Vue SFC REPL `@vue/repl` : u can edit, run and preview the code effect display in real time on the web page.",
  "author": "andurils",
  "scripts": {
    "lint": "vue-cli-service lint",
    "type:check": "vue-tsc --noEmit",
    "type:build": "vue-tsc --declaration --emitDeclarationOnly",
    "bootstrap": "cnpm i || yarn",
    "clean": "rimraf node_modules",
    "deploy:build": "vue-cli-service build --mode deploy",
    "dev": "vue-cli-service serve ",
    "dev:play": "vue-cli-service serve --mode play",
    "dist": "vue-cli-service build  --mode production --target lib --name vue-code-viewer --dest lib src/index.ts"
  },
  "main": "lib/vue-code-viewer.common.js",
  "unpkg": "lib/vue-code-viewer.umd.js",
  "dependencies": {
    "deepmerge": "^4.2.2",
    "normalize.css": "^8.0.1",
    "tiny-sass-compiler": "^0.12.2"
  },
  "devDependencies": {
    "@iconify/vue2": "^1.1.3",
    "@types/codemirror": "^5.60.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.15",
    "@vue/cli-service": "~4.5.0",
    "@vue/composition-api": "^1.4.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vueuse/core": "^5.3.0",
    "ant-design-vue": "^1.7.8",
    "autoprefixer": "^9.8.8",
    "babel-eslint": "^10.1.0",
    "codemirror": "^5.62.3",
    "compression-webpack-plugin": "^6.1.1",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.5",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "highlight.js": "^11.3.1",
    "less": "^4.1.2",
    "markdown-it": "^12.2.0",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate": "^1.5.2",
    "postcss": "^7.0.39",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^8.0.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tua-body-scroll-lock": "^1.2.1",
    "typescript": "^4.6.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.5",
    "unplugin-vue2-script-setup": "^0.10.2",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-router": "^3.2.0",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.32.1"
  },
  "peerDependencies": {
    "vue": ">= 2.5 < 3"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ],
  "homepage": "https://andurils.github.io/vue-code-view",
  "keywords": [
    "vue",
    "components",
    "codemirror",
    "code editor"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
